class Solution
{
	public:
		string is_palindrome(int n)
		{
		    // Code here.
		    int x=n;
		    int rev=0;
		    while(n>0){
		        int remainder=n%10;
		        rev= rev*10+remainder;
		        n/=10;
		      }
		    if(rev==x)
		     cout<<"Yes";
		    else 
		     cout<<"No";
		    
		}
};
